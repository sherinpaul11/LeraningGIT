package day1;

public class SmallestInArray {
	public static void main(String[] args) {
		int[] A = {11,4,67,1,118,2,12};
		int s=0;
		for(int i=0;i< A.length;i++)
		{
			for (int j=i+1;j<A.length;j++) {
				
				if(A[i]<A[j]) {
					s=A[i];
				}
				else {
					s=A[j];
				}
			
			}
		}
		System.out.println(s);
	}
}

